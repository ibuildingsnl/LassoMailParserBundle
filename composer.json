{
    "name": "lasso/mail-parser-bundle",
    "description": "Parse mails using the Laminas framework libraries",
    "type": "symfony-bundle",
    "keywords": [
        "mail", "email", "parsing", "Symfony2", "zend"
    ],
    "homepage": "https://github.com/lassodatasystems",
    "license": "BSD-3-Clause",
    "require": {
        "laminas/laminas-mail": "^2.16",
        "laminas/laminas-zendframework-bridge": "^1.5"
    },
    "autoload": {
        "psr-0": { "Lasso\\MailParserBundle": "" }
    },
    "target-dir": "Lasso/MailParserBundle",
    "require-dev": {
        "ext-mbstring": "*",
        "phpunit/phpunit": "^9.0.0"
    }
}
