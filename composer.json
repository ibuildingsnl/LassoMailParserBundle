{
    "name": "lasso/mail-parser-bundle",
    "description": "Parse mails using the Zend framework libraries",
    "type": "symfony-bundle",
    "keywords": [
        "mail", "email", "parsing", "Symfony2", "zend"
    ],
    "homepage": "https://github.com/lassodatasystems",
    "license": "BSD-3-Clause",
    "require": {
        "laminas/laminas-mail": "^2.12"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.zendframework.com/"
        }
    ],
    "autoload": {
        "psr-0": { "Lasso\\MailParserBundle": "" }
    },
    "target-dir": "Lasso/MailParserBundle",
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    }
}
